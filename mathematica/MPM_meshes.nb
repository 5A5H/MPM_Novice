Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Return", " ", "\"\<internal points\>\"", " ", "of", " ", "a", " ", 
    "given", " ", "quadrilateral", " ", "with", " ", "volume"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"GP4", "[", "XI_", "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "SHP", ",", "\[Xi]", ",", "\[Eta]", ",", "X", ",", "a", ",", "A"}], 
        "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"SHP", "=", 
         RowBox[{
          RowBox[{"1", "/", "4"}], 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"1", "-", "\[Xi]"}], ")"}], 
             RowBox[{"(", 
              RowBox[{"1", "-", "\[Eta]"}], ")"}]}], ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"1", "+", "\[Xi]"}], ")"}], 
             RowBox[{"(", 
              RowBox[{"1", "-", "\[Eta]"}], ")"}]}], ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"1", "+", "\[Xi]"}], ")"}], " ", 
             RowBox[{"(", 
              RowBox[{"1", "+", "\[Eta]"}], ")"}]}], ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"1", "-", "\[Xi]"}], ")"}], " ", 
             RowBox[{"(", 
              RowBox[{"1", "+", "\[Eta]"}], ")"}]}]}], "}"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"X", "=", 
         RowBox[{"SHP", ".", "XI"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"a", "=", "0.5"}], ";", 
        RowBox[{"Sqrt", "[", 
         RowBox[{"1", "/", "3"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"A", "=", 
         RowBox[{
          RowBox[{"Area", "[", 
           RowBox[{"Polygon", "[", "XI", "]"}], "]"}], "/", "4"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Return", "[", 
         RowBox[{"N", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"X", "/.", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"\[Xi]", "\[Rule]", 
                  RowBox[{"-", "a"}]}], ",", 
                 RowBox[{"\[Eta]", "\[Rule]", 
                  RowBox[{"-", "a"}]}]}], "}"}]}], ",", 
              RowBox[{"X", "/.", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"\[Xi]", "\[Rule]", "a"}], ",", 
                 RowBox[{"\[Eta]", "\[Rule]", 
                  RowBox[{"-", "a"}]}]}], "}"}]}], ",", 
              RowBox[{"X", "/.", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"\[Xi]", "\[Rule]", "a"}], ",", 
                 RowBox[{"\[Eta]", "\[Rule]", "a"}]}], "}"}]}], ",", 
              RowBox[{"X", "/.", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"\[Xi]", "\[Rule]", 
                  RowBox[{"-", "a"}]}], ",", 
                 RowBox[{"\[Eta]", "\[Rule]", "a"}]}], "}"}]}]}], "}"}], ",", 
            
            RowBox[{"{", 
             RowBox[{"A", ",", "A", ",", "A", ",", "A"}], "}"}]}], "}"}], 
          "]"}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"A", " ", "regular", " ", "Grid", " ", "generator"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"RegularGrid", "[", 
      RowBox[{"w_", ",", "h_", ",", "dx_"}], "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"XI", ",", "NElmtX", ",", "NElmtY", ",", "Elmt"}], "}"}], ",",
        "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"XI", "=", 
         RowBox[{"Partition", "[", 
          RowBox[{
           RowBox[{"Flatten", "[", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i", ",", "j"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "0", ",", "h", ",", "dx"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "0", ",", "w", ",", "dx"}], "}"}]}], "]"}], 
            "]"}], ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"NElmtX", ",", "NElmtY"}], "}"}], "=", 
         RowBox[{"IntegerPart", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"w", "/", "dx"}], ",", 
            RowBox[{"h", "/", "dx"}]}], "}"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Elmt", "=", 
         RowBox[{"Partition", "[", 
          RowBox[{
           RowBox[{"Flatten", "[", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"{", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"j", "-", "1"}], ")"}], 
                  RowBox[{"(", 
                   RowBox[{"NElmtX", "+", "1"}], ")"}]}], "+", 
                 RowBox[{"(", 
                  RowBox[{"i", "-", "1"}], ")"}], "+", "1"}], 
                "\[IndentingNewLine]", ",", 
                RowBox[{
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"j", "-", "1"}], ")"}], 
                  RowBox[{"(", 
                   RowBox[{"NElmtX", "+", "1"}], ")"}]}], "+", 
                 RowBox[{"(", 
                  RowBox[{"i", "-", "1"}], ")"}], "+", "2"}], 
                "\[IndentingNewLine]", ",", 
                RowBox[{
                 RowBox[{"j", 
                  RowBox[{"(", 
                   RowBox[{"NElmtX", "+", "1"}], ")"}]}], "+", 
                 RowBox[{"(", 
                  RowBox[{"i", "-", "1"}], ")"}], "+", "2"}], 
                "\[IndentingNewLine]", ",", 
                RowBox[{
                 RowBox[{"j", 
                  RowBox[{"(", 
                   RowBox[{"NElmtX", "+", "1"}], ")"}]}], "+", 
                 RowBox[{"(", 
                  RowBox[{"i", "-", "1"}], ")"}], "+", "1"}]}], 
               "\[IndentingNewLine]", "}"}], "\[IndentingNewLine]", ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "NElmtX"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "NElmtY"}], "}"}]}], "]"}], "]"}], ",", 
           "4"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Return", "[", 
