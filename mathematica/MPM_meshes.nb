Cell[BoxData[
 RowBox[{
  RowBox[{"ParticleX2D", "[", "toelementmesh_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "MeshElements", ",", "MeshCoord", ",", "NoElements", ",", "PX", ",", 
      "PV"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"MeshCoord", "=", 
      RowBox[{"toelementmesh", "[", "\"\<Coordinates\>\"", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"MeshElements", "=", 
      RowBox[{
       RowBox[{"toelementmesh", "[", "\"\<MeshElements\>\"", "]"}], 
       "\[LeftDoubleBracket]", 
       RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"NoElements", "=", 
      RowBox[{"Length", "[", "MeshElements", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"Table", "[", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Compute", " ", "Particle", " ", "Position", " ", "PX", " ", 
         RowBox[{"(", 
          RowBox[{"element", " ", "center"}], ")"}]}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"PX", "=", 
          RowBox[{"Mean", "[", 
           RowBox[{"MeshCoord", "\[LeftDoubleBracket]", 
            RowBox[{
            "MeshElements", "\[LeftDoubleBracket]", "i", 
             "\[RightDoubleBracket]"}], "\[RightDoubleBracket]"}], "]"}]}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{"Compute", " ", "Particle", " ", "Position", " ", "PX", " ", 
           RowBox[{"(", 
            RowBox[{"element", " ", "center"}], ")"}]}], "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"PV", "=", 
          RowBox[{"Area", "[", 
           RowBox[{"Polygon", "[", 
            RowBox[{"MeshCoord", "\[LeftDoubleBracket]", 
             RowBox[{
             "MeshElements", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}], "\[RightDoubleBracket]"}], "]"}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
          "Output", " ", "as", " ", "3", "d", " ", "coordinates", " ", 
           "followed", " ", "by", " ", "volume"}], "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
           "PX", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], ",", 
           
           RowBox[{
           "PX", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], ",", 
           "0.0", ",", "PV"}], "}"}]}], "\[IndentingNewLine]", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "NoElements"}], "}"}]}], "]"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
